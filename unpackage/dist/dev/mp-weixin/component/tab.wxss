
image{
  display: block;
}
.tabBox{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 96rpx;
  background:#fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  box-shadow:0px -4rpx 8rpx 0rpx rgba(0,0,0,0.11);
  z-index: 99;
  left: 0rpx;
}
.tabList{
  text-align: center;
  font-size: 24rpx;
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
  color: #D61313;
}
.tabImg{
  width: 48rpx;
  height: 48rpx;
  margin: 0 auto;
}
.tabListColor{
  color:rgba(191,191,191,1);
}

